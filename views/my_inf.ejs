          <tbody>
            <tr>
              <th scope="row">ID</th>
              <td></td>
              <td>
                <input type="text" id="Company_Name" value="<%=req.session.MyCompanyInfo[0]['Comp_NAME']%>" style="text-align: center; width: 100%" />
              </td>
            </tr>
            <tr>
              <th scope="row">대표자 성명</th>
              <td></td>
              <td>
                <input
                  type="text"
                  id="President_NAME"
                  value="<%=req.session.MyCompanyInfo[0]['President_NAME']%>"
